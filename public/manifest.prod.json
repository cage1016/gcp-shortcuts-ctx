{
  "description": "Add support to the omnibox to quick launch Google Cloud Platform.",
  "name": "GCP Console Shortcuts",
  "background": {
    "page": "background.html"
  },
  "omnibox": {
    "keyword": "gcp"
  },
  "permissions": [
    "identity",
    "storage",
    "https://accounts.google.com/*",
    "https://*.googleapis.com/*",
    "https://*.cloud.google.com/*"
  ],
  "version": "0.1",
  "minimum_chrome_version": "9",
  "manifest_version": 2,
  "oauth2": {
    "client_id": "493281202705-s2tjqeh470qfkmtam691uboa64phiqad.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/cloud-platform.read-only"
    ]
  }
}