{
  "description": "Add support to the omnibox to quick launch Google Cloud Platform Console.",
  "name": "GCP Shortcuts",
  "background": {
    "page": "background.html"
  },
  "omnibox": {
    "keyword": "g"
  },
  "permissions": [
    "identity",
    "storage",
    "https://accounts.google.com/*",
    "https://*.googleapis.com/*",
    "https://*.cloud.google.com/*"
  ],
  "version": "0.1",
  "minimum_chrome_version": "9",
  "manifest_version": 2,
  "oauth2": {
    "client_id": "1005992922714-bgv55u50icm7k1nvshck56co6if1m9ae.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/cloud-platform.read-only"
    ]
  }
}